<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sri.ai.praise.sgsolver.demo.SGSolverDemoController">
	<top>
		<BorderPane style="-fx-background-color: white" BorderPane.alignment="CENTER">
			<top>
				<ToolBar style="-fx-background-color: null;" BorderPane.alignment="CENTER">
					<items>
						<Button fx:id="openMenuButton" mnemonicParsing="false" text="Open Menu">
							<tooltip>
								<Tooltip text="Open Menu" />
							</tooltip>
						</Button>
						<Separator orientation="VERTICAL" />
						<Button fx:id="newButton" mnemonicParsing="false" text="New">
							<tooltip>
								<Tooltip text="New" />
							</tooltip>
						</Button>
						<Button fx:id="openFileButton" mnemonicParsing="false" text="Open">
							<tooltip>
								<Tooltip text="Open File" />
							</tooltip>
						</Button>
						<Button fx:id="saveButton" mnemonicParsing="false" text="Save">
							<tooltip>
								<Tooltip text="Save" />
							</tooltip>
						</Button>
						<Separator orientation="VERTICAL" />
						<Label text="Example" />
						<ComboBox fx:id="examplesComboBox" onAction="#exampleSelected" prefWidth="300.0" />
						<Separator orientation="VERTICAL" />
						<Button fx:id="addPageButton" mnemonicParsing="false" text="+" onAction="#addModelPage">
							<tooltip>
								<Tooltip text="Add Page" />
							</tooltip>
						</Button>
						<Button fx:id="removePageButton" mnemonicParsing="false" text="-" onAction="#removeModelPage">
							<tooltip>
								<Tooltip text="Remove Page" />
							</tooltip>
						</Button>
						<Separator orientation="VERTICAL" />
					</items>
				</ToolBar>
			</top>
		</BorderPane>
	</top>
   <center>
      <Pagination fx:id="modelPagination" pageCount="0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
